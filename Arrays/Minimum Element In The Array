# Write a program to return the minimum element in the array.

// Method 1
def find_minimum(arr, length):
    """Function to find the minimum in the array return the minimum value"""
    return min(arr)
    
 def main():
    n = int(input())
    arr=[]
    for i in range(n):
        arr.append(int(input()))
    print(find_minimum(arr, n))
    
main()


// Method 2
def min_element(n):
   minimum = arr[0]
   for  j in range(n):
      if (arr[j]<minimum):
          minimum = arr[j]
   return minimum
    
    
n = int(input()) 
arr = []
for i in range(n):
    arr.append(int(input()))
print(min_element(n))



// Method 3
def minimum(n):
  arr.sort()
  return arr[0]

n = int(input()) 
arr = []
for i in range(n):
    arr.append(int(input()))
print(minimum(n))



// Method 4
def minimum(n):
  arr.sort(reverse = True)
  return arr[-1]

n = int(input()) 
arr = []
for i in range(n):
    arr.append(int(input()))
print(minimum(n))


















